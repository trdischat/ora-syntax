{
  "name": "ora-template",
  "displayName": "Online Repository Assistant",
  "description": "Language support for templating in the Online Repository Assistant (ORA)",
  "version": "0.0.2",
  "publisher": "trdischat",
  "license": "./LICENSE.md",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/trdischat/ora-syntax"
  },
  "homepage": "https://github.com/trdischat/ora-syntax",
  "bugs": {
    "url": "https://github.com/trdischat/ora-syntax/issues"
  },
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "ora",
    "template",
    "genealogy"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onLanguage:ora-template"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ora-template",
        "aliases": [
          "ORA Template"
        ],
        "extensions": [
          ".ora-template"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ora-template",
        "scopeName": "source.ora-template",
        "path": "./syntaxes/ora.tmLanguage.json"
      }
    ]
  }
}